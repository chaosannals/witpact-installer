{
    "version": "0.0.1",
    "name": "chenshenchao/witpact-installer",
    "description": "Yet a composer installer of WitPact.",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "chenshenchao",
            "email": "chenshenchao@outlook.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "WitPact\\Composer\\": "main"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test"
        }
    },
    "require": {
        "composer-plugin-api": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "extra": {
        "class": "WitPact\\Composer\\Plugin"
    },
    "scripts": {
        "test": [
            "phpunit --bootstrap vendor/autoload.php --testdox test"
        ]
    }
}
